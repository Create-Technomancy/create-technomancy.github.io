@unocss;

.bg-img-technomancy {
    background-image: url(./assets/background.png);
    background-origin: content-box;
    background-position: center;
    background-attachment: fixed;
    background-size: cover;
    background-repeat: no-repeat;
}

.blog-post {
    a {
        @apply text-blue-4;
    }
}

.__markdoc-blog-section {
    h2 {
        @apply mb-0;
    }

    p {
        @apply italic mt-0;
    }
}
